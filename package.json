{
  "name": "smartq",
  "version": "1.0.0",
  "description": "Smart queue application",
  "main": "app.js",
  "scripts": {
    "start": "node -r dotenv/config app.js",
    "server": "nodemon --exec babel-node -r dotenv/config app.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "lint": "concurrently \"eslint .\" \"npm run lint --prefix client\"",
    "lint:fix": "concurrently \"eslint . --fix\" \"npm run lint:fix --prefix client\""
  },
  "keywords": [
    "react",
    "javascript",
    "full",
    "stack",
    "node",
    "js",
    "express",
    "mongo"
  ],
  "author": "Mark Smirnov, Lukianchuk Maxim",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.1",
    "mongoose": "^5.8.7",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "concurrently": "^5.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.1",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1"
  }
}
